{
  "name": "polaris-interview-agent",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "backend:install": "cd poc && pip install -r requirements_fastapi.txt",
    "backend:dev": "cd poc && python start_fastapi_server.py",
    "backend:https": "cd poc && python start_https_server.py",
    "backend:ssl": "cd poc && python generate_ssl_cert.py",
    "backend:logs": "cd poc && tail -f fastapi_server.log",
    "backend:clean": "cd poc && rm -rf __pycache__ && rm -f *.log",
    "backend:setup": "npm run backend:install && npm run backend:ssl",
    "backend:start": "node scripts/backend-manager.js start",
    "backend:stop": "node scripts/backend-manager.js stop",
    "backend:status": "node scripts/backend-manager.js status",
    "backend:manager": "node scripts/backend-manager.js",
    "dev:full": "concurrently \"npm run dev\" \"npm run backend:dev\"",
    "dev:https": "concurrently \"npm run dev\" \"npm run backend:https\"",
    "dev:managed": "concurrently \"npm run dev\" \"npm run backend:start\"",
    "deploy:vercel": "vercel --prod",
    "deploy:railway": "cd poc && railway up",
    "deploy:render": "echo 'Deploy to Render via dashboard'",
    "test:api": "curl http://localhost:3000/api/interview/health",
    "test:fastapi": "curl http://localhost:8000/health"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.1",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@react-three/drei": "^10.6.1",
    "@react-three/fiber": "^9.3.0",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/auth-ui-react": "^0.4.7",
    "@supabase/auth-ui-shared": "^0.1.8",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.53.0",
    "@types/three": "^0.179.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "gsap": "^3.13.0",
    "jsrepo": "^2.4.5",
    "lucide-react": "^0.536.0",
    "next": "15.4.5",
    "ogl": "^1.0.11",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.62.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "three": "^0.179.1",
    "zod": "^4.0.14"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^9.1.0",
    "eslint": "^9",
    "eslint-config-next": "15.4.5",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.3.6",
    "typescript": "^5"
  }
}
